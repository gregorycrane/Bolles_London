<ROW><CELL><num value="40">40</num></CELL><CELL>. . .</CELL><CELL>Bunhill Fields . . . . . . . . .</CELL><CELL></CELL><CELL></CELL><CELL><num value="199">199</num></CELL><CELL><num value="276">276</num></CELL><CELL><num value="4159">4,159</num></CELL><CELL><num value="9525">9,525</num></CELL><CELL><num value="13960">13,960</num></CELL><CELL><num value="45">45</num></CELL><CELL><num value="15306">15,306</num></CELL><CELL><num value="1725">1,725</num></CELL><CELL><num value="17031">17,031</num></CELL><CELL><num value="55">55</num></CELL><CELL><num value="30991">30,991</num></CELL></ROW>
 
