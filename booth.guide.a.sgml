<ROW><CELL><num value="11">11</num></CELL><CELL>. . .</CELL><CELL>West Brompton . . . . . . . . .</CELL><CELL></CELL><CELL></CELL><CELL><num value="103">103</num></CELL><CELL><num value="75">75</num></CELL><CELL><num value="771">771</num></CELL><CELL><num value="3532">3,532</num></CELL><CELL><num value="4378">4,378</num></CELL><CELL><num value="12">12</num>.<num value="6">6</num></CELL><CELL><num value="18429">18,429</num></CELL><CELL><num value="11834">11,834</num></CELL><CELL><num value="30263">30,263</num></CELL><CELL><num value="87">87</num>.<num value="4">4</num></CELL><CELL><num value="34641">34,641</num></CELL></ROW>
 
<ROW><CELL>a.</CELL><CELL>B-<num value="9">9</num>.</CELL><CELL>E. Selwood Terrace.</CELL><CELL>This is very similar in character to the previous blocks, consisting mainly of good houses, some newly erected, and the stables appertaining. a few smaller houses in Thistle Grove Lane, the quiet, narrow turning forming the western boundary.</CELL><CELL></CELL><CELL></CELL><CELL><num value="0">0</num></CELL><CELL><num value="0">0</num></CELL><CELL><num value="0">0</num></CELL><CELL><num value="0">0</num></CELL><CELL><num value="0">0</num></CELL><CELL><num value="940">940</num></CELL><CELL><num value="1217">1,217</num></CELL><CELL><num value="2157">2,157</num></CELL><CELL><num value="100">100</num></CELL><CELL><num value="2157">2,157</num></CELL></ROW>
 
