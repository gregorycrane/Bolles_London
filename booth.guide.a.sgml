<ROW><CELL>b.</CELL><CELL>H-<num value="3">3</num>.</CELL><CELL>E. East Road, North Road.</CELL><CELL>This is a large thickly populated block interspersed with workshops and other business premises. Some busy shopkeepers, and many good mechanics form the larger half of the inhabitants, the remainder belonging to the many grades of unskilled labour.</CELL><CELL></CELL><CELL></CELL><CELL><num value="352">352</num></CELL><CELL><num value="1818">1,818</num></CELL><CELL><num value="7375">7,375</num></CELL><CELL><num value="9545">9,545</num></CELL><CELL><num value="44">44</num>.<num value="2">2</num></CELL><CELL><num value="11105">11,105</num></CELL><CELL><num value="923">923</num></CELL><CELL><num value="12028">12,028</num></CELL><CELL><num value="55">55</num>.<num value="8">8</num></CELL><CELL><num value="21573">21,573</num></CELL></ROW>
 
