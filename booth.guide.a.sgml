<ROW><CELL><num value="39">39</num></CELL><CELL>. . .</CELL><CELL>Belsize Park. . . . . . . . . .</CELL><CELL></CELL><CELL></CELL><CELL><num value="52">52</num></CELL><CELL><num value="0">0</num></CELL><CELL><num value="323">323</num></CELL><CELL><num value="3604">3,604</num></CELL><CELL><num value="3927">3,927</num></CELL><CELL><num value="11">11</num>.<num value="7">7</num></CELL><CELL><num value="17095">17,095</num></CELL><CELL><num value="12415">12,415</num></CELL><CELL><num value="29510">29,510</num></CELL><CELL><num value="88">88</num>.<num value="3">3</num></CELL><CELL><num value="33437">33,437</num></CELL></ROW>
 
